Algoritmo sin_titulo
	Escribir 'Introduzca 1 numero del 1 al 3'
	leer A
		
	Si A = 1 Entonces
		Escribir 'Numero 1'
	SiNo
		Si A = 2 Entonces
			Escribir 'Numero 2'
		SiNo
			Si A = 3
				Escribir 'Numero3'
				
			SiNo
				Escribir 'No ha intruducido un valor permitido[1-3]'
			FinSi
			
		FinSi
			
	FinSi
FinAlgoritmo
